{
	"name": "cs-podcast-player",
	"version": "0.1.0",
	"homepage": "",
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"prop-types": "^15.7.2",
		"react": "^16.14.0",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^16.10.2",
		"react-redux": "^7.1.1",
		"react-scripts": "^3.2.0",
		"react-transition-group": "^4.3.0",
		"redux": "^4.0.4"
	},
	"publishConfig": {
		"@honey-ctrlspire:registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"start": "react-scripts start && npm run watch:css",
		"buildx": "react-scripts build",
		"buildy": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files",
		"build": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.js",
		"build:css": "postcss src/styles/styles.scss -o src/styles/app.css",
		"watch:css": "postcss src/styles/styles.scss -o src/styles/app.css -w",
		"deploy": "cp ./build/index.html ./build/404.html && gh-pages -d build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.17.10",
		"@babel/preset-env": "^7.17.10",
		"@reduxjs/toolkit": "^1.8.1",
		"autoprefixer": "^9.8.8",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"gh-pages": "^2.1.1",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^4.12.0",
		"postcss-cli": "^9.1.0",
		"postcss-loader": "^3.0.0",
		"sass-loader": "^7.2.0",
		"tailwindcss": "^3.0.24",
		"webpack-cli": "^4.9.2"
	}
}
